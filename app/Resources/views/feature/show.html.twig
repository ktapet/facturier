{% extends 'base.html.twig' %}

{% block body %}
    <h1>Feature</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feature.id }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ feature.value }}</td>
            </tr>
            <tr>
                <th>Datcre</th>
                <td>{% if feature.datCre %}{{ feature.datCre|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datupd</th>
                <td>{% if feature.datUpd %}{{ feature.datUpd|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('feature_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('feature_edit', { 'id': feature.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
