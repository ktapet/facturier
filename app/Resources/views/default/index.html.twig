{# app/Resources/views/default/index.html.twig #}
{% extends 'base.html.twig' %}
 
{% block body %}
<h3>Exemplu - afisare date in tabel</h3>
<div class="table-responsive">
    <table class="table table-hover records_list">
        <thead>
            <tr>
            {% for val in entities[0] %}
                <th>{{ val }}</th>
            {% endfor %}
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
            <tr>
                {% for val in entity %}
                    <td>{{ val }}</td>
                {% endfor %}
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}